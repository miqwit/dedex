           xmlns:iso4217a="http://ddex.net/xml/20120404/iso4217a"
           xmlns:iso3166a2="http://ddex.net/xml/20120404/iso3166a2"
           targetNamespace="http://ddex.net/xml/20120404/ddex"
           elementFormDefault="unqualified"
           attributeFormDefault="unqualified">
   <xs:import namespace="http://ddex.net/xml/20120404/iso639a2"  schemaLocation="http://ddex.net/xml/20120404/iso639a2.xsd"/>
   <xs:import namespace="http://ddex.net/xml/20120404/iso4217a"  schemaLocation="http://ddex.net/xml/20120404/iso4217a.xsd"/>
 <xs:import namespace="http://ddex.net/xml/20120404/iso3166a2"  schemaLocation="http://ddex.net/xml/20120404/iso3166a2.xsd"/> 
