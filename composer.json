{
    "type": "library",
    "name": "miqwit/dedex",
    "description": "Parse DDEX ERN files and provide a convenient API to use it",
	  "keywords": ["ddex", "parsing", "music", "xml"],
	  "homepage": "https://github.com/miqwit/dedex",
	  "authors": [
        {
            "name": "MickaÃ«l Arcos",
            "role": "Developer"
        }
		],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "goetas-webservices/xsd2php-runtime": "v0.2.16",
        "goetas-webservices/xsd2php": "^0.4.4",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "DedexBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DedexBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {}
}
